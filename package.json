{
  "name": "adonis-fullstack-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "start": "run-p start:adonis start:gulp",
    "start:adonis": "adonis serve --dev",
    "start:server": "nodemon --inspect server.js",
    "start:gulp": "gulp",
    "start:webpack": "webpack --watch",
    "start:laravel-mix": "NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
    "test": "node ace test",
    "build:laravel-mix": "NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/session": "^1.0.27",
    "@adonisjs/shield": "^1.0.8",
    "@adonisjs/validator": "^5.0.6",
    "@fortawesome/fontawesome-free": "^5.15.0",
    "bootstrap-icons": "^1.0.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "adonis-pug": "^4.2.0",
    "autoprefixer": "^9.8.6",
    "bootstrap": "^5.0.0-alpha1",
    "cssnano": "^4.1.10",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "laravel-mix": "^6.0.0-alpha.1",
    "mysql": "^2.18.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.0.0-beta.33"
  },
  "autoload": {
    "App": "./app"
  }
}
